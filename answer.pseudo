<?php

//@param name for get name
function getNameAndReturn( $name ) {
	
	//generate number between 6 and 15
	$genNumber 	= rand( 6, 15 );
	
	//concatenate name and generated number
	$nameNumber = $name . $genNumber;
	
	//return name along with number
	return $nameNumber;
}

echo getNameAndReturn("Rahul");

?>
